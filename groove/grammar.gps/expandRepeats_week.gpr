            </attr>
        </edge>
        <edge from="n2" to="n2">
            <attr name="label">
                <string>let:start = int:p0.start+(int:p0.start+604800*(int:p0.duration*(int:p0.repeats-1)))</string>
            </attr>
        </edge>
        <edge from="n2" to="n1">
            <attr name="label">
                <string>timing</string>
            </attr>
        </edge>
